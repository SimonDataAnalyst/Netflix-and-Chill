SELECT show_id, COUNT(*)
FROM netflix
GROUP BY show_id
HAVING COUNT(*) > 1;
DELETE FROM netflix
WHERE ROWID NOT IN (
  SELECT MIN(ROWID)
  FROM netflix
  GROUP BY show_id
);
